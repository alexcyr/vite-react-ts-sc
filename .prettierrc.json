{
	"printWidth": 100,
	"trailingComma": "all",
	"tabWidth": 2,
	"semi": true,
	"singleQuote": false,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"endOfLine": "auto",
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"useTabs": true,
	"htmlWhitespaceSensitivity": "css"
}
